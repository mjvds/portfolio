{
  "name": "personal",
  "version": "0.0.0",
  "scripts": {
    "build:prod": "ng build --configuration production --base-href /portfolio/",
    "deploy": "npx angular-cli-ghpages --dir=dist/portfolio/browser/",
    "build:deploy": "npm run build:prod && npm run deploy"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.4",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@ng-icons/core": "^33.0.0",
    "@ng-icons/devicon": "^33.1.0",
    "@ng-icons/jam-icons": "^33.0.0",
    "@ng-icons/material-icons": "^33.1.0",
    "gsap": "^3.14.2",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "@biomejs/biome": "^2.3.7",
    "@tailwindcss/postcss": "^4.1.12",
    "angular-cli-ghpages": "^3.0.2",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
